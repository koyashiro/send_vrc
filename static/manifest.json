{
  "manifest_version": 2,
  "name": "SendVRC | VRChatにURLを送るやつ",
  "version": "1.0.5",
  "description": "Sends the URL to VRChat. send_vrc_desktop is required for one-click operation.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "this page send to VRChat"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": ["send.js"]
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "clipboardRead",
    "http://localhost:11400/*"
  ]
}
